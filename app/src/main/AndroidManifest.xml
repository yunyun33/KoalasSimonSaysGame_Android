<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yui.koalassimonsaysgame_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.KoalasSimonSaysGame_android">
        <activity android:name="com.yui.koalassimonsaysgame_android.totalScorePage.TotalScoreActivity"></activity>
        <activity android:name="com.yui.koalassimonsaysgame_android.playGamePage.PlayGameActivity" />
        <activity android:name="com.yui.koalassimonsaysgame_android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>